{
	"name": "pipeline_API",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['country']"
								},
								"sink": {
									"name": "country",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['country abbreviation']"
								},
								"sink": {
									"name": "country abbreviation",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['post code']"
								},
								"sink": {
									"name": "post code",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['places'][0]['place name']"
								},
								"sink": {
									"name": "place name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['places'][0]['longitude']"
								},
								"sink": {
									"name": "longitude",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['places'][0]['latitude']"
								},
								"sink": {
									"name": "latitude",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['places'][0]['state']"
								},
								"sink": {
									"name": "state",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['places'][0]['state abbreviation']"
								},
								"sink": {
									"name": "state abbreviation",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['places']"
					}
				},
				"inputs": [
					{
						"referenceName": "API",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_PARAM",
						"type": "DatasetReference",
						"parameters": {
							"CONTAINER": "data",
							"FOLDER": "Output",
							"FILENAME": "claim_api.csv"
						}
					}
				]
			}
		],
		"annotations": []
	}
}