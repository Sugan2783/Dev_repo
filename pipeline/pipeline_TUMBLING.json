{
	"name": "pipeline_TUMBLING",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"modifiedDatetimeStart": {
								"value": "@pipeline().parameters.pipeline_startdate",
								"type": "Expression"
							},
							"modifiedDatetimeEnd": {
								"value": "@pipeline().parameters.pipeline_enddate",
								"type": "Expression"
							},
							"wildcardFolderPath": "data",
							"wildcardFileName": "*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "tumb",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_PARAM",
						"type": "DatasetReference",
						"parameters": {
							"CONTAINER": "landing",
							"FOLDER": {
								"value": " ",
								"type": "Expression"
							},
							"FILENAME": {
								"value": " ",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pipeline_startdate": {
				"type": "string",
				"defaultValue": "@triggeroutputs().windowstarttime"
			},
			"pipeline_enddate": {
				"type": "string",
				"defaultValue": "@triggeroutputs().windowendtime"
			}
		},
		"folder": {
			"name": "ADV_PRACTICE"
		},
		"annotations": []
	}
}