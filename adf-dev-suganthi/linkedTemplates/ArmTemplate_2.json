						"source(output(",
						"          INCIDENT_KEY as integer,",
						"          INCIDENT_ID as integer,",
						"          INCIDENT_LOCATION as string,",
						"          INCIDENT_DATE as date,",
						"          INCIDENT_DESCRIPTION as string,",
						"          INCIDENT_TYPE as string,",
						"          FLAG as string,",
						"          EFF_DATE as date,",
						"          END_DATE as date",
						"     ),",
